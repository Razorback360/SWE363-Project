{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-jsdoc"],
  "printWidth": 80,
  "tabWidth": 2,
  "endOfLine": "lf",
  "useTabs": false,
  "trailingComma": "all",
  "semi": true,
  "quoteProps": "as-needed",
  "singleQuote": true,
  "importOrder": ["^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
